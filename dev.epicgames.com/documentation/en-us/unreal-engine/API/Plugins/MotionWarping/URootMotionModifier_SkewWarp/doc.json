ider\"\u003eEWarpPointAnimProvider\u003c/a\u003e InWarpPointAnimProvider,\u003cbr\u003e\nUPARAM(DisplayName=\"Warp Point Anim Transform\") FTransform InWarpPointAnimTransform,\u003cbr\u003e\nUPARAM(DisplayName=\"Warp Point Anim \u003ca href=\"https://dev.epicgames.com/documentation/en-us/unreal-engine/API/Plugins/MotionWarping/EWarpPointAnimProvider\"\u003eBone\u003c/a\u003e Name\") FName InWarpPointAnimBoneName,\u003cbr\u003e\nUPARAM(DisplayName=\"Warp Translation\") bool bInWarpTranslation,\u003cbr\u003e\nUPARAM(DisplayName=\"Ignore Z Axis\") bool bInIgnoreZAxis,\u003cbr\u003e\nUPARAM(DisplayName=\"Warp Rotation\") bool bInWarpRotation,\u003cbr\u003e\nUPARAM(DisplayName=\"Rotation Type\") \u003ca href=\"https://dev.epicgames.com/documentation/en-us/unreal-engine/API/Plugins/MotionWarping/EMotionWarpRotationType\"\u003eEMotionWarpRotationType\u003c/a\u003e InRotationType,\u003cbr\u003e\nUPARAM(DisplayName=\"Rotation Method\") \u003ca href=\"https://dev.epicgames.com/documentation/en-us/unreal-engine/API/Plugins/MotionWarping/EMotionWarpRotationMethod\"\u003eEMotionWarpRotationMethod\u003c/a\u003e InRotationMethod,\u003cbr\u003e\nUPARAM(DisplayName=\"Warp Rotation Time Multiplier\") float InWarpRotationTimeMultiplier,\u003cbr\u003e\nUPARAM(DisplayName=\"Warp Max Rotation Rate\") float InWarpMaxRotationRate\u003cbr\u003e\n )\u003c/p\u003e\n\u003c/div\u003e\u003c/td\u003e\n      \u003ctd\u003e \u003c/td\u003e\n    \u003c/tr\u003e\n    \u003ctr\u003e\n      \u003ctd\u003e\u003cimg src=\"https://d1iv7db44yhgxn.cloudfront.net/documentation/images/63daf8f7-18f3-4486-81c5-1f2d6e05185c/api_function_public.png\" alt=\"Public function\" loading=\"lazy\" title=\"Public function\" convert=\"false\" width=\"18\" /\u003e \u003cimg src=\"https://d1iv7db44yhgxn.cloudfront.net/documentation/images/43520302-f6e4-4d6b-b4ba-52865b83515b/api_function_const.png\" alt=\"Const\" loading=\"lazy\" title=\"Const\" convert=\"false\" width=\"18\" /\u003e\u003c/td\u003e\n      \u003ctd\u003eFTra